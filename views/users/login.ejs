<% layout('layouts/boilerplate')%>
<h1>Login</h1>
<form action="" method="post"></form>
<form action="/login" method="post" novalidate class="validated-form">
  <div class="mb-3">
    <label for="username" class="form-label">Username</label>
    <input
      type="text"
      id="username"
      class="form-control"
      name="username"
      required
    />
    <div class="valid-feedback">Good Soup</div>
  </div>
  <div class="mb-3">
    <label for="password" class="form-label">Password</label>
    <input
      type="password"
      name="password"
      id="password"
      class="form-control"
      required
    />
    <div class="valid-feedback">Good Soup</div>
  </div>
  <button class="btn btn-success">Login</button>
</form>
